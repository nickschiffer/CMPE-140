# $a0 = a, $a1 = b, $s0 = c, $s1 = x, $s2 = y

addiu $a0, $0, 0x8000
addiu $a1, $0, 0x00A9
addiu $s0, $0, 1974

mult $a0, $a0			# x=a*a;
mflo $s1			# store the value of x to memory location 0x20

mult $s1,  $a1			# y=x*b;
mfhi $s2			# store the value of y to memory location 0x24

srl $s2, $s2, 16		# y=y>>16;

# c=(c+y/c)/2;

addu $t0, $s0, $s2		# c + y
div $t0, $s0			# c+y/c
addi $t1, $0, 2			# $t1 = 2
div $t0, $t1			# (c+y/c)/2
addu $s0, $0, $t1		# $s0 = $t1				

addi $t2, $0, 1665		# $t2 = 1665

while: 				# while(c >= 1665){ c = (c + y / c) / 2;}
	slt  $t3, $t2, $s0   	# if $t2 < $0, $t3 = 1 else $t3 = 0 
	bne  $t3, $0, L1 	# $t3 = 1 branch to L1
	sll $s0, $s0, 8		# c=c<<8;
	j done

L1:				# c=(c+y/c)/2;
	addu $t0, $s0, $s2	# c + y
	div $t0, $s0		# c+y/c
	addi $t1, $0, 2		# $t1 = 2
    	div $t0, $t1		# (c+y/c)/2
    	addu $s0, $0, $t1	# $s0 = $t1
    	j while  

done: 	#sw $v0, 30($s0)		# store the value of c to memory location 0x30